{
  "name": "react-native-breeze",
  "version": "0.0.3-alpha-12",
  "description": "Utility-first style framework for React Native projects",
  "main": "dist/commonjs/index.js",
  "scripts": {
    "test": "jest",
    "prepare": "bob build; npm run copy:plugin",
    "copy:plugin": "cpr ./dist/commonjs/plugin ./plugin -d"
  },
  "keywords": [
    "react",
    "native",
    "tailwindcss",
    "breeze"
  ],
  "author": "Erick Tamayo",
  "license": "ISC",
  "dependencies": {
    "@babel/types": "^7.11.5",
    "babel-template": "^6.26.0",
    "clear-module": "^4.1.1",
    "cpr": "^3.0.1",
    "deepmerge": "^4.2.2",
    "fast-memoize": "^2.5.2",
    "is-plain-object": "^5.0.0",
    "lodash.get": "^4.4.2",
    "react-native": "^0.63.3",
    "stealthy-require": "^1.1.1",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@react-native-community/bob": "^0.16.2",
    "@types/babel-core": "^6.25.6",
    "@types/jest": "^26.0.14",
    "@types/lodash.get": "^4.4.6",
    "@types/react": "^16.9.50",
    "@types/react-native": "^0.63.23",
    "@types/tinycolor2": "^1.4.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.3.0",
    "babel-preset-expo": "^8.3.0",
    "babel-preset-react-native": "^4.0.1",
    "cpy-cli": "^3.1.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "react-native": "src/index.ts",
  "module": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "files": [
    "src",
    "dist",
    "plugin",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "@react-native-community/bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ]
}
